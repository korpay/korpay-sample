{
  "expo": {
    "name": "react-native-expo-sample",
    "slug": "react-native-expo-sample",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "reactnativeexposample",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "ispmobile",
          "kb-acp",
          "kbbank",
          "cloudpay",
          "supertoss",
          "mpocket.online.ansimclick",
          "monimopay",
          "monimopayauth",
          "shinhan-sr-ansimclick",
          "hdcardappcardansimclick",
          "nhallonepayansimclick",
          "nhappcardansimclick",
          "lotteappcard",
          "com.wooricard.wcard",
          "newsmartpib",
          "citimobileapp",
          "yonseipay",
          "payco",
          "kakaotalk",
          "naversearchthirdlogin",
          "lpayapp",
          "naversearchapp",
          "lmslpay"
        ]
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.reactnativeexposample",
      "appleTeamId": "5HW35FJGSZ"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.reactnativeexposample"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}